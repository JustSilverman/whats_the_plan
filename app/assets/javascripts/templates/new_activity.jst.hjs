<div class="new-activity">
  <a class="new-close-icon" href="#">X</a>
  {{#if meal}}
    <i class="activity-icon icon-food icon-2x"></i>
  {{else}}
    <i class="activity-icon icon-camera-retro icon-2x"></i>
  {{/if}}
  <h4 class="new-destination">{{destination}}</h4>
  <p class="new-address">{{address}}</p>
</div>
